{
  "title": "MPD",
  "rules": [
    {
      "description": "Player",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f6"
          },
          "to": [
            {
              "shell_command": "~/bin/player"
            }
          ]
        }
      ]
    },
    {
      "description": "Previous",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f7"
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/mpc prev"
            }
          ]
        }
      ]
    },
    {
      "description": "Toggle",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f8"
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/mpc toggle"
            }
          ]
        }
      ]
    },
    {
      "description": "Next",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f9"
          },
          "to": [
            {
              "shell_command": "/usr/local/bin/mpc next"
            }
          ]
        }
      ]
    }
  ]
}
